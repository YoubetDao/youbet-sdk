{
  "private": true,
  "scripts": {
    "build-watch:sdk": "pnpm --filter youbet-sdk build-watch",
    "dev:simple-react": "pnpm --filter simple-react dev",
    "dev-all:simple-react": "run-p build-watch:sdk dev:simple-react",
    "build:sdk": "pnpm --filter youbet-sdk build",
    "build:simple-react": "pnpm --filter simple-react build",
    "build-all:simple-react": "pnpm build:sdk && pnpm build:simple-react"
  },
  "devDependencies": {
    "inquirer": "^9.3.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "youbet-sdk": "^0.0.8"
  }
}